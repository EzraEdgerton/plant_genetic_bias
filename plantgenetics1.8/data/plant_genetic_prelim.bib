@article{ ISI:000384964700020,
Author = {Li, Hao-Sen and Hoffmann, Ary A. and Guo, Jing-Feng and Zuo, Yun and
   Xue, Xiao-Feng and Pang, Hong and Hong, Xiao-Yue},
Title = {{Identification of two lineages of host-associated eriophyoid mites
   predisposed to different levels of host diversification}},
Journal = {{MOLECULAR PHYLOGENETICS AND EVOLUTION}},
Year = {{2016}},
Volume = {{105}},
Pages = {{235-240}},
Month = {{DEC}},
Abstract = {{Herbivorous arthropods can diversify as a consequence of evolutionary
   changes in response to their plant hosts. Current patterns of host
   association of herbivores are likely to reflect a long evolutionary
   history of herbivore-plant co-evolution. Here, we used molecular
   phylogenetics to track the evolutionary history of host shifts and
   diversification of 66 eriophyoid mites (Acari, Eriophyoidea), and linked
   past patterns of evolutionary diversification to more recent patterns of
   divergence by tracking population genetic variation in 13 of the
   eriophyoid mite species feeding on different gymnosperm hosts. This
   allowed us to explore the relationship between a past history of
   diversification and the current potential of mites to undergo host range
   shifts. We found that population-level diversity across gymnosperm hosts
   as measured by 28S rRNA markers was greater in species from the mite
   Glade that had radiated across evolutionary time to utilize a variety of
   hosts including angiosperms, compared to species from the Glade that has
   remained restricted to ancestral gymnosperm hosts. Species from the
   radiated Glade exhibited higher variation in host use. Lineages of mites
   that have in the past been able to radiate and adapt to diverse plants
   may therefore be predisposed to continue their expansion on new hosts,
   although additional Glades need to be tested. (C) 2016 Elsevier Inc. All
   rights reserved.}},
Language = {{English}},
DOI = {{10.1016/j.ympev.2016.09.010}},
ISSN = {{1055-7903}},
EISSN = {{1095-9513}},
Keywords = {{Stabilization; Diversification; Host association; Eriophyoid mites;
   Co-evolution}},
Keywords-Plus = {{WHEAT CURL MITE; INSECT HERBIVORES; GENETIC DIVERSITY; MOLECULAR CLOCK;
   SPECIALIZATION; EVOLUTION; ACARI; MACROEVOLUTION; ACARIFORMES;
   SPECIFICITY}},
