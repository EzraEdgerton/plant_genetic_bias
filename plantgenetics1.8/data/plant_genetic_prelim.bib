@article{ ISI:000384268200002,
Author = {Mihoub, Imane and Ghashghaie, Jaleh and Badeck, Franz W. and Robert,
   Thierry and Lamothe-Sibold, Marlene and Aid, Fatiha},
Title = {{Intraspecific variability of carbon isotope discrimination and its
