@article{ ISI:000388035800001,
Author = {Xiong, Fangjie and Dong, Pan and Liu, Mei and Xie, Gengxin and Wang, Kai
   and Zhuo, Fengping and Feng, Li and Yang, Lu and Li, Zhengguo and Ren,
   Maozhi},
Title = {{Tomato FK506 Binding Protein 12KD (FKBP12) Mediates the Interaction
   between Rapamycin and Target of Rapamycin (TOR)}},
Journal = {{FRONTIERS IN PLANT SCIENCE}},
Year = {{2016}},
Volume = {{7}},
Month = {{NOV 18}},
Abstract = {{Target of Rapamycin (TOR) signaling is an important regulator in
   multiple organisms including yeast, plants, and animals. However, the
   TOR signaling in plants is much less understood as compared to that in
   yeast and animals. TOR kinase can be efficiently suppressed by rapamycin
   in the presence of functional FK506 Binding Protein 12 KD (FKBP12) in
   yeast and animals. In most examined higher plants rapamycin fails to
   inhibit TOR kinase due to the non-functional FKBP12. Here we find that
   tomato plants showed obvious growth inhibition when treated with
   rapamycin and the inhibitory phenotype is similar to suppression of TOR
   causing by active-site TOR inhibitors (asTORis) such as KU63794,
   AZD8055, and Torin1. The chemical genetic assays using TOR inhibitors
   and heterologous expressing SIFKBP12 in Arabidopsis indicated that the
   TOR signaling is functional in tomato. The protein gel shifting and TOR
   inhibitors combination assays showed that SIFKBP12 can mediate the
   interaction between rapamycin and TOR. Furthermore, comparative
   expression profile analysis between treatments with rapamycin and
   KU63794 identified highly overlapped Differentially Expressed Genes
   (DEGs) which are involved in many anabolic and catabolic processes, such
   as photosynthesis, cell wall restructuring, and senescence in tomato.
   These observations suggest that SIFFBP12 is functional in tomato. The
   results provided basic information of TOR signaling in tomato, and also
   some new insights into how TOR controls plant growth and development
   through reprogramming the transcription profiles.}},
Language = {{English}},
DOI = {{10.3389/fpls.2016.01746}},
Article-Number = {{1746}},
ISSN = {{1664-462X}},
Keywords = {{tomato; target of rapamycin; SIFKBP12; rapamycin; KU63794; gene
   expression profile}},
Keywords-Plus = {{CELL-GROWTH CONTROL; PLANT-GROWTH; ARABIDOPSIS-THALIANA; FUNCTIONAL
   GENOMICS; KINASE; METABOLISM; EXPRESSION; YEAST; COMPLEX; MTOR}},
