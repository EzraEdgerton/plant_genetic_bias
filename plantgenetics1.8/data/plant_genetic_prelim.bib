@article{ ISI:000386756500002,
Author = {Rani, Reema and Yadav, Prashant and Barbadikar, Kalyani M. and Baliyan,
   Nikita and Malhotra, Era Vaidya and Singh, Binay Kumar and Kumar, Arun
   and Singh, Dhiraj},
Title = {{CRISPR/Cas9: a promising way to exploit genetic variation in plants}},
Journal = {{BIOTECHNOLOGY LETTERS}},
Year = {{2016}},
Volume = {{38}},
Number = {{12}},
Pages = {{1991-2006}},
Month = {{DEC}},
Abstract = {{Creation of variation in existing gene pool of crop plants is the
   foremost requirement in crop improvement programmes. Genome editing is a
   tool to produce knock out of target genes either by introduction of
   insertion or by deletion that disrupts the function of a specific gene.
   The CRISPR/Cas9 (clustered regularly interspaced short palindromic
   repeats/CRISPR-associated protein 9) system is the most recent addition
   to the toolbox of sequence-specific nucleases that includes ZFNs and
   TALENs. The CRISPR/Cas9 system allows targeted cleavage of genomic DNA
   guided by a small noncoding RNA, resulting in gene modifications by both
   non-homologous end joining and homology-directed repair mechanisms.
   Here, we present an overview of mechanisms of CRISPR, its potential
   roles in creating variation in germplasm and applications of this novel
   interference pathway in crop improvement. The availability of the
   CRISPR/Cas9 system holds promise in facilitating both forward and
   reverse genetics and will enhance research in crops that lack genetic
   resources.}},
Language = {{English}},
DOI = {{10.1007/s10529-016-2195-z}},
ISSN = {{0141-5492}},
EISSN = {{1573-6776}},
Keywords = {{CRISPR/Cas9; Mutagenesis; Genome editing; Crop improvement}},
Keywords-Plus = {{SITE-DIRECTED MUTAGENESIS; BACTERIAL IMMUNE-SYSTEM; ZINC-FINGER
   NUCLEASES; TARGETED MUTAGENESIS; GUIDE RNA; ARABIDOPSIS-THALIANA; TRAIT
   STACKING; 1ST GENERATION; CAS SYSTEM; GENOME}},
Web-of-Science-Categories  = {{Biotechnology \& Applied Microbiology}},
