@article{ ISI:000387585900003,
Author = {Xiao, Shanshan and Shao, Min and Wang, Dong and Li, Wenqi and Liu,
   Fengquan},
Title = {{Identification and Evolution of FYVE Domain-Containing Proteins and
   Their Expression Patterns in Response to Abiotic Stresses in Rice}},
Journal = {{PLANT MOLECULAR BIOLOGY REPORTER}},
Year = {{2016}},
Volume = {{34}},
Number = {{6}},
Pages = {{1064-1082}},
Month = {{DEC}},
Abstract = {{The FYVE domain is a typical zinc finger motif containing four conserved
   CxxC pairs and has been shown to specifically binds to PtdIns(3)P on the
   surface of cell membrane. FYVE domain-containing proteins are commonly
   distributed in eukaryotic cells and have been implicated in diverse
   functions like signal transduction, membrane trafficking, exocytosis and
   endocytosis, phosphoinositides (PIs) metabolism, and cytoskeletal
   regulation. Analysis of the rice genome using comprehensive online
   databases and research tools resulted in the identification of 19
   putative rice FYVE (OsFVYE) proteins. Based on domain architectural and
   phylogenetic analyses, these OsFYVEs were further classified into six
   groups. Groups I-V were conserved in Arabidopsis (dicots) and rice
   (monocot) at both the genetic and protein levels, while group VI was
   widely present in plants but truncated in Arabidopsis and species of
   Brassicaceae. Comprehensive and comparative investigation of their
   expression profiles showed that FYVE genes in plant exhibited a variety
   of expression patterns during different developmental stages and in
   response to phytohormones and abiotic stresses. These findings indicated
   that both OsFYVE and AtFYVE genes may played potential roles in normal
   plant growth, hormone signal transduction, and abiotic stress tolerance.
   Results from our study shed light on the potential roles of FYVE
   proteins in plant growth, development, and stress responses.}},
Language = {{English}},
DOI = {{10.1007/s11105-016-0988-9}},
ISSN = {{0735-9640}},
EISSN = {{1572-9818}},
Keywords = {{FYVE domain; Rice; Gene expression; Abiotic stress}},
Keywords-Plus = {{GUANINE-NUCLEOTIDE EXCHANGE; PHOSPHATIDYLINOSITOL 3-PHOSPHATE;
   FACIOGENITAL DYSPLASIA; MEMBRANE TRAFFICKING; ARABIDOPSIS-THALIANA;
   CRYSTAL-STRUCTURE; EARLY ENDOSOMES; ANKYRIN REPEAT; PLANT-CELLS;
   AUTOPHAGOSOME BIOGENESIS}},
