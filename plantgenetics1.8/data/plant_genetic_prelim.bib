@article{ ISI:000387673700005,
Author = {Jiang, Libo and Ye, Meixia and Zhu, Sheng and Zhai, Yi and Xu, Meng and
   Huang, Minren and Wu, Rongling},
Title = {{Computational identification of genes modulating stem height-diameter
   allometry}},
Journal = {{PLANT BIOTECHNOLOGY JOURNAL}},
Year = {{2016}},
Volume = {{14}},
Number = {{12}},
Pages = {{2254-2264}},
Month = {{DEC}},
Abstract = {{The developmental variation in stem height with respect to stem diameter
   is related to a broad range of ecological and evolutionary phenomena in
   trees, but the underlying genetic basis of this variation remains
   elusive. We implement a dynamic statistical model, functional mapping,
   to formulate a general procedure for the computational identification of
   quantitative trait loci (QTLs) that control stem height-diameter
   allometry during development. Functional mapping integrates the
   biological principles underlying trait formation and development into
   the association analysis of DNA genotype and endpoint phenotype, thus
   providing an incentive for understanding the mechanistic interplay
   between genes and development. Built on the basic tenet of functional
   mapping, we explore two core ecological scenarios of how stem height and
   stem diameter covary in response to environmental stimuli: (i) trees
   pioneer sunlit space by allocating more growth to stem height than
   diameter and (ii) trees maintain their competitive advantage through an
   inverse pattern. The model is equipped to characterize pioneering' QTLs
   (piQTLs) and maintaining' QTLs (miQTLs) which modulate these two
   ecological scenarios, respectively. In a practical application to a
   mapping population of full-sib hybrids derived from two Populus species,
   the model has well proven its versatility by identifying several piQTLs
   that promote height growth at a cost of diameter growth and several
   miQTLs that benefit radial growth at a cost of height growth. Judicious
   application of functional mapping may lead to improved strategies for
   studying the genetic control of the formation mechanisms underlying
   trade-offs among quantities of assimilates allocated to different growth
   parts.}},
Language = {{English}},
DOI = {{10.1111/pbi.12579}},
ISSN = {{1467-7644}},
EISSN = {{1467-7652}},
Keywords = {{height-diameter allometry; mathematical equation; functional mapping;
   quantitative trait loci}},
Keywords-Plus = {{GENOME-WIDE ASSOCIATION; QUANTITATIVE TRAIT LOCI; FULL-SIB FAMILY;
   COMPLEX TRAITS; PLANT HEIGHT; TRANSGENIC POPLARS; PINUS-SYLVESTRIS;
   BAYESIAN LASSO; TREE HEIGHT; RAIN-FOREST}},
