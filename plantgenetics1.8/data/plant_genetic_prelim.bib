@article{ ISI:000386408400001,
Author = {Mokhtari, Hamid and Sepahvand, Mehdi and Fasihfar, Ahmad},
Title = {{Thermoeconomic and exergy analysis in using hybrid systems (GT plus MED
   plus RO) for desalination of brackish water in Persian Gulf}},
Journal = {{DESALINATION}},
Year = {{2016}},
Volume = {{399}},
Pages = {{1-15}},
Month = {{DEC 1}},
Abstract = {{In this paper, a Gas Turbine + Multi Effect Desalination + Reverse
   Osmosis (GT + MED + RO) system is designed with respect to Siemens gas
   turbines, to provide water and electricity for Bashagerd County, located
   in the south of Iran (in the outskirts of Persian Gulf). There is 79.2 x
   10(6) (kWh/year) and 103043 (m(3)/day) of electricity and water demand,
   respectively. Siemens GT model 400 (SGT400) is selected to provide
   electricity in the place. It is seen in MED system that there is a major
   difference between desalinated and required water. But, as we have high
   rates of flow in rejected water from MED condenser to the gulf, an RO
   system is used in rejected outflow of the MED system. By doing so, more
   fresh water is derived from the whole system. In this design, the
   recovery of RO system is 42 \%, and final price 2.3 \$ per cubic meter.
   By using GT + MED + RO hybrid system, not only the required electricity,
   but also enough fresh water can be produced for this County. The hybrid
   system had some added costs. And the final price is reduced from 2.8 to
   2.3 \$/m(3) by using the excess electricity to run the RO pump. (C) 2016
   Elsevier B.V. All rights reserved.}},
Language = {{English}},
DOI = {{10.1016/j.desal.2016.07.044}},
